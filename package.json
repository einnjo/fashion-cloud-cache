{
    "name": "fashion-cloud-cache",
    "version": "0.0.0",
    "main": "dist/index.js",
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc",
        "start": "node dist/index.js | pino-pretty-min",
        "dev": "tsc-watch --onSuccess 'yarn start'",
        "test": "tap --ts --no-cov",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\"",
        "lint": "eslint",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/node": "15.0.1",
        "@types/tap": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^6.0.0",
        "pino-pretty-min": "^0.3.2",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "tap": "^15.0.9",
        "tsc-watch": "^4.2.9",
        "typescript": "4.2.4"
    },
    "dependencies": {
        "dotenv": "^9.0.2",
        "envalid": "^7.1.0",
        "express": "^4.17.1",
        "pino": "^6.11.3"
    },
    "description": "Fashion Cloud's Cache API"
}
